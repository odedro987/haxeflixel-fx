{
	"lineEnds": {
		"leftCurly": "both",
		"rightCurly": "both",
		"objectLiteralCurly": {
			"leftCurly": "after"
		}
	},
	"sameLine": {
		"ifElse": "next",
		"doWhile": "next",
		"tryBody": "next",
		"tryCatch": "next"
	},
	"wrapping": {
        "methodChain": {
            "defaultWrap": "onePerLine"
        },
		"maxLineLength": 160,
		"callParameter": {"defaultWrap": "noWrap"},
 		"functionSignature": {"defaultWrap": "noWrap"}
    }
	
}
